Algoritmo "Porcentagem da soma"

Var
   numero1, numero2,  porcento : real

Inicio
      escreva("Digite dois numero: ")
      leia(numero1, numero2)
      
      porcento <- (numero1 + numero2) * (45/100)
      
      escreva("A porcentagem da soma de ",numero1, " + ",numero2, " e ",porcento)


Fimalgoritmo